  "scripts": {
    "test": "nyc --reporter=text-lcov npm test | coveralls"
  },
  "type": "module",
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.3.10",
    "coveralls": "^3.1.1",
    "esm": "^3.2.25",
